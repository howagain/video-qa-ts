{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/src/index.js": {
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ],
  "redirects": [{ "source": "/", "destination": "/api", "permanent": false }]
}